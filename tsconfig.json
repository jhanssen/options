{
    "compilerOptions": {
        "target": "es2022",
        "newLine": "LF",
        "module": "es6",
        "moduleResolution": "node",
        "pretty": true,
        "noImplicitAny": true,
        "declaration": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "downlevelIteration": true,
        "strict": true,
        "sourceMap": true,
        "incremental": true,
        "lib": ["es2022"],
        "outDir": "./dist"
    }
}
